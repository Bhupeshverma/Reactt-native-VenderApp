A;AAAA;AACE;AAAA;AAAA;AAAA;AAAA,uBADF;AAEE;AAAA;AAAA,0BAAU,WAAU,UAApB;AAAgCnC,6BAAKoC;AAArC;AAFF,qBA5BF;AAiCE;AAAA;AAAA;AACE;AAAA;AAAA;AAAA;AAAA,uBADF;AAEE;AAAA;AAAA,0BAAU,WAAU,UAApB;AAAgCpC,6BAAKqC;AAArC;AAFF,qBAjCF;AAsCE;AAAA;AAAA;AACE;AAAA;AAAA;AAAA;AAAA,uBADF;AAEE;AAAA;AAAA,0BAAU,WAAU,UAApB;AAAgCrC,6BAAKsC;AAArC;AAFF,qBAtCF;AA2CE;AAAA;AAAA;AACE;AAAA;AAAA;AAAA;AAAA,uBADF;AAEE;AAAA;AAAA,0BAAU,WAAU,UAApB;AAAgCtC,6BAAKuC;AAArC;AAFF,qBA3CF;AAgDE;AAAA;AAAA;AACE;AAAA;AAAA;AAAA;AAAA,uBADF;AAEE;AAAA;AAAA,0BAAU,WAAU,UAApB;AAAgCvC,6BAAKwC;AAArC;AAFF,qBAhDF;AAqDE;AAAA;AAAA;AACE;AAAA;AAAA;AAAA;AAAA,uBADF;AAEE;AAAA;AAAA,0BAAU,WAAU,UAApB;AAAgCxC,6BAAKyC;AAArC;AAFF,qBArDF;AA0DE;AAAA;AAAA;AACE;AAAA;AAAA;AAAA;AAAA,uBADF;AAEE;AAAA;AAAA,0BAAU,WAAU,UAApB;AAAgCzC,6BAAK0C;AAArC;AAFF,qBA1DF;AA+DE;AAAA;AAAA;AACE;AAAA;AAAA;AAAA;AAAA,uBADF;AAEE;AAAA;AAAA,0BAAU,WAAU,UAApB;AAAgC1C,6BAAK2C;AAArC;AAFF;AA/DF,mBAFF;AAyED,iBA3EI,CAAP;AA4EC,eA9EG,CAAR;AA+EG,aAjFT;AAFJ,WAZN;AAqGG,eAAKtD,KAAL,CAAWE,IAAX,CAAgB6B,MAAhB,IAA0B,CAA1B,GACC;AAAA;AAAA;AACE,yBAAU,MADZ;AAEG,qBAAO,EAACwB,iBAAiB,MAAlB,EAA0B5D,QAAQ,EAAlC,EAFV;AAGG,uBAAS;AAAA,uBAAI,OAAK6D,WAAL,EAAJ;AAAA,eAHZ;AAIE;AAAA;AAAA,gBAAM,OAAO,EAACnB,OAAO,MAAR,EAAgBD,YAAY,KAA5B,EAAb;AAAA;AAAA;AAJF,WADD,GAMW;AA3Gd;AAFF,OADJ;AAmHD;;;;;AAEH,SAAUqB,eAAV,CAA0BxD,KAA1B,EAAgC;AAC5B,SAAO;;AAEHC,UAAMD,MAAMyD,gBAFT;AAGHjD,cAAUR,MAAM0D;AAHb,GAAP;AAKH;kBACc,yBAAQF,eAAR,EAAwB,EAAChC,mBAAD,EAAOmC,qCAAP,EAAsBC,qCAAtB,EAAqCC,iCAArC,EAAxB,EAA2E/D,YAA3E,C;;AACf,IAAM4B,SAAS,wBAAWoC,MAAX,CAAkB;AAC7BnC,aAAW;AACPI,UAAM,CADC;AAEPa,mBAAe,QAFR;AAGPX,oBAAgB,cAHT;AAIPqB,qBAAiB;;AAJV,GADkB;AAQ7B1B,OAAK;AACDlC,YAAQG,UADP,EARwB;;AAY7BgC,QAAM;AACFE,UAAM,EADJ;AAEFuB,qBAAiB;;AAFf,GAZuB;AAiB7BS,WAAS;AACPhC,UAAK,CADE;AAEPC,gBAAY;AAFL;AAjBoB,CAAlB,CAAf","sourcesContent":["import React, { Component } from 'react';\nimport {View,StyleSheet,Text, Dimensions, ScrollView, ActivityIndicator,ToastAndroid} from 'react-native';\nimport { connect } from 'react-redux';\nimport { Sync, SavedProducts, ClearProducts, SyncWithCrm} from '../actions';\nimport {SearchBar } from 'react-native-elements';\nimport { Card, CardContent, CardImage } from 'react-native-material-cards'\nimport {Actions} from 'react-native-router-flux'\nimport {  Image ,Button, Row ,Subtitle,Caption, Heading, Title} from '@shoutem/ui';\nimport { MaterialDialog } from 'react-native-material-dialog';\n\nvar { height, width } = Dimensions.get('window');\nvar box_count = 1;\nvar box_height = height / box_count;\nclass DetailScreen extends Component {\n    constructor(props){\n        super(props);\n        this.state={\n            data:[],\n            total_count:0,\n            visible:true\n        }\n\n    }\n   componentDidMount(){\n     var dataArray, count=0;\n       console.log(\"data\",this.props.data)\n       console.log(\"syncData\", this.props.syncData);\n       this.props.data.map((item,index)=>{\n         return item.map((item,index)=>{\n           return(\n             item.map((item,index)=>{\n               count+=item.total\n             })\n           )\n         })\n       })\n       console.log(count);\n       this.setState({total_count: count})\n   }\n   _handleSync(){\n     var syncArray;\n     var date =new Date()\n     var dd = date.getDate();\n     var mm = date.getMonth()+1; //January is 0!\n     var yyyy = date.getFullYear();\n     if(dd<10){\n          dd='0'+dd;\n      }\n      if(mm<10){\n          mm='0'+mm;\n    }\n     var today = yyyy+'-'+dd+'-'+mm\n     console.log(\"today\", today);\n     this.props.syncData.map((item,index)=>{\n       syncArray =item.map((item,index)=>{\n         return(\n           item.map((item,index)=>{\n             return item\n           })\n         )\n       })\n     })\n     console.log(\"syncArray\",syncArray);\n     this.props.Sync(syncArray,today, this.state.total_count)\n     Actions.nextScreen()\n}\n\n  render(){\n    return(\n        <View style={styles.container}>\n\n          <View style={[styles.box, styles.box1]}>\n\n              {this.props.data.length == 0 ?\n                <View style={{flex:1,\n                  alignItems: 'center',\n                  justifyContent: 'center'\n                  }}>\n                    <Text style={{fontSize:20, fontWeight: \"bold\", color: \"#555\"}}>\n                      The Tray is Empty !!\n                    </Text>\n                  </View>\n                :\n                <ScrollView>\n                  {\n                    this.props.data.map((itemss, index)=>{\n\n                          return  itemss.map((items, value)=>{\n\n                              return items.map((item, index)=>{\n\n                                  return(\n\n                                    <View styleName=\"vertical\" key={item.product_id}>\n                                      <Row>\n                                        {item.product_image === null ?\n                                          <Image\n                                           styleName=\"medium rounded-corners\"\n                                           source={{ uri: 'https://shoutem.github.io/img/ui-toolkit/examples/image-1.png'}}\n                                         />:\n                                         <Image\n                                          styleName=\"medium rounded-corners\"\n                                          source={{ uri: item.product_image}}\n                                        />}\n                                        <View styleName=\"vertical stretch space-between\">\n                                          <Subtitle>{item.name}</Subtitle>\n                                          <View style={{flexDirection: 'row' ,justifyContent: 'space-around'}}>\n                                            <Subtitle>{item.product_id}</Subtitle>\n                                            <Subtitle>{item.total}</Subtitle>\n                                          </View>\n                                        </View>\n                                      </Row>\n                                      <Row>\n                                        <Title>SIZES</Title>\n                                        <Title styleName=\"h-center\">SCAN-COUNT</Title>\n                                      </Row>\n                                      <Row>\n                                        <Subtitle>Size_34</Subtitle>\n                                        <Subtitle styleName=\"h-center\">{item.size_34}</Subtitle>\n\n                                      </Row>\n                                      <Row >\n                                        <Subtitle>Size_35</Subtitle>\n                                        <Subtitle styleName=\"h-center\">{item.size_35}</Subtitle>\n\n                                      </Row>\n                                      <Row >\n                                        <Subtitle>Size_36</Subtitle>\n                                        <Subtitle styleName=\"h-center\">{item.size_36}</Subtitle>\n\n                                      </Row>\n                                      <Row >\n                                        <Subtitle>Size_37</Subtitle>\n                                        <Subtitle styleName=\"h-center\">{item.size_37}</Subtitle>\n\n                                      </Row>\n                                      <Row >\n                                        <Subtitle>Size_38</Subtitle>\n                                        <Subtitle styleName=\"h-center\">{item.size_38}</Subtitle>\n\n                                      </Row>\n                                      <Row >\n                                        <Subtitle>Size_39</Subtitle>\n                                        <Subtitle styleName=\"h-center\">{item.size_39}</Subtitle>\n\n                                      </Row>\n                                      <Row >\n                                        <Subtitle>Size_40</Subtitle>\n                                        <Subtitle styleName=\"h-center\">{item.size_40}</Subtitle>\n\n                                      </Row>\n                                      <Row >\n                                        <Subtitle>Size_41</Subtitle>\n                                        <Subtitle styleName=\"h-center\">{item.size_41}</Subtitle>\n\n                                      </Row>\n                                      <Row >\n                                        <Subtitle>Size_42</Subtitle>\n                                        <Subtitle styleName=\"h-center\">{item.size_42}</Subtitle>\n\n                                      </Row>\n                                  </View>\n\n                                )\n                                })\n                              })\n                            })\n                }\n              </ScrollView>\n            }\n\n\n            {this.props.data.length != 0 ?\n              <Button\n                styleName=\"dark\"\n                 style={{backgroundColor: \"#000\", height: 40}}\n                 onPress={()=>this._handleSync()}>\n                <Text style={{color: '#fff', fontWeight: '400'}} >SYNC WITH CRM</Text>\n              </Button>:null}\n          </View>\n\n        </View>\n    )\n  }\n}\nfunction  mapStateToProps(state){\n    return {\n\n        data: state.savedDataReducer,\n        syncData: state.SaveSyncFormatReducer\n    }\n}\nexport default connect(mapStateToProps,{Sync, SavedProducts, ClearProducts, SyncWithCrm})(DetailScreen);\nconst styles = StyleSheet.create({\n    container: {\n        flex: 1,\n        flexDirection: 'column',\n        justifyContent: 'space-around',\n        backgroundColor: '#eff6f7',\n\n    },\n    box: {\n        height: box_height //set this one\n    },\n    //header\n    box1: {\n        flex: 12,\n        backgroundColor: '#eff6f7',\n\n    },\n    spinner: {\n      flex:1,\n      alignItems: 'center',\n    }\n})\n"]}]