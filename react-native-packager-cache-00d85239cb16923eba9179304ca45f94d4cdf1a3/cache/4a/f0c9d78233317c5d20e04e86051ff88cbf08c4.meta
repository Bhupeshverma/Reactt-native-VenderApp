["07389940d6e0412a5c965636c9b1ae2c9201afa0","e40e587bf89a8dcb436147578efdffc76e26ccda",["react","./View","../drivers/DriverShape"],[139,229,268],{"version":3,"sources":["/home/ziddi/Desktop/venders/node_modules/@shoutem/animation/src/animations/HeroHeader.js"],"names":["HeroHeader","props","driver","children","propTypes","isRequired","PropTypes","node"],"mappings":";;;;;;;AAAA;;;;AACA;;AACA;;IA0BaA,U,WAAAA,U;;;;;;;;;;6BAQF;AAAA,mBACsB,KAAKC,KAD3B;AAAA,UACCC,MADD,UACCA,MADD;AAAA,UACSC,QADT,UACSA,QADT;;;AAGP,aACE;AAAA;AAAA,UAAM,eAAc,MAApB,EAA2B,QAAQD,MAAnC;AACGC;AADH,OADF;AAKD;;;4BAfMC,S,GAAY;AACjBF,UAAQ,yBAAYG,UADH;;AAKjBF,YAAU,gBAAMG,SAAN,CAAgBC;AALT,C","sourcesContent":["import React, { Component } from 'react';\nimport { View } from './View';\nimport { DriverShape } from '../drivers/DriverShape';\n/*\n * HeroHeader adds complex effect to its children components.\n * Connect it to driver to animate it.\n * e.g.:\n * ...\n * const driver = new ScrollDriver();\n *\n * return (\n *    <Screen styleName=\"full-screen\">\n *      <HeroHeader driver={driver}>\n *          <Image />\n *      </HeroHeader>\n *      <ScrollView\n *        {...driver.scrollViewProps}\n *      >\n *        <Title>Title</Title>\n *      </ScrollView>\n *    </Screen>\n * );\n *\n * ...\n * Above code will create scroll dependent parallax animation over Image component\n * where image will be scrolled 1.5 times faster than Title and the image will have\n * a zoom in effect when the scroll reaches the top of the screen (on bounce)\n */\nexport class HeroHeader extends Component {\n  static propTypes = {\n    driver: DriverShape.isRequired,\n    /**\n     * Components to which an effect will be applied\n     */\n    children: React.PropTypes.node,\n  }\n  render() {\n    const { driver, children } = this.props;\n\n    return (\n      <View animationName=\"hero\" driver={driver}>\n        {children}\n      </View>\n    );\n  }\n}\n"]}]