;AAAA,QAAO,OAAM,yBAAb;AACE;AAAC,4BAAD;AAAA;AACE;AACE,yBACE;AAAA;AAAA;AACE,yDAAM,MAAK,OAAX;AADF,WAFJ;AAME,iBAAM,OANR;AAOE,0BACE;AAAA;AAAA,cAAQ,WAAU,OAAlB;AACE;AAAA;AAAA;AAAA;AAAA;AADF;AARJ;AADF;AADF,KA1LF;AA2ME;AAAA;AAAA,QAAO,OAAM,2BAAb;AACE;AAAC,4BAAD;AAAA;AACE;AACE,yBACE;AAAA;AAAA;AACE;AAAA;AAAA;AAAA;AAAA;AADF,WAFJ;AAME,iBAAM,OANR;AAOE,0BACE;AAAA;AAAA;AACE;AAAA;AAAA;AAAA;AAAA;AADF;AARJ;AADF;AADF,KA3MF;AA4NE;AAAA;AAAA,QAAO,OAAM,sCAAb;AACE;AAAC,4BAAD;AAAA;AACE;AACE,yBACE;AAAA;AAAA;AACE;AAAA;AAAA;AAAA;AAAA;AADF,WAFJ;AAME,iBAAM,OANR;AAOE,0BACE;AAAA;AAAA,cAAQ,WAAU,OAAlB;AACE;AAAA;AAAA;AAAA;AAAA;AADF;AARJ;AADF;AADF,KA5NF;AA6OE;AAAA;AAAA,QAAO,OAAM,0CAAb;AACE;AAAC,4BAAD;AAAA,UAAsB,OAAO,EAAEJ,iBAAiB,SAAnB,EAA7B;AACE;AACE,qBAAU,OADZ;AAEE,yBACE;AAAA;AAAA;AACE,yDAAM,MAAK,MAAX;AADF,WAHJ;AAOE,iBAAM,OAPR;AAQE,iBAAO;AACLE,kBAAM,0BADD;AAELC,kBAAM,kBAFD;AAGLC,mBAAO;AAHF;AART;AADF;AADF;AA7OF,GADF;AAkQD","sourcesContent":["import React from 'react';\nimport { Dimensions } from 'react-native';\n\nimport { View } from '../../components/View';\nimport { Stage } from './Stage';\nimport {\n  Heading,\n  NavigationBar,\n  Title,\n  Text,\n  Image,\n  Button,\n  Icon,\n  DropDownMenu,\n} from '../../index';\n\nconst window = Dimensions.get('window');\n\nfunction NavBarStageContainer(props) {\n  return (\n    <View\n      {...props}\n      style={{\n        width: window.width,\n        height: 70,\n        ...props.style,\n      }}\n    />\n  );\n}\n\nconst navBarDropDownOptions = [\n  { name: 'All', value: 1 },\n  { name: 'Sport', value: 1 },\n  { name: 'World', value: 1 },\n  { name: 'Lifestyle', value: 1 },\n  { name: 'Food', value: 1 },\n  { name: 'Music', value: 1 },\n  { name: 'Movies', value: 1 },\n  { name: 'Tech', value: 1 },\n  { name: 'Fun', value: 1 },\n  { name: 'Fashion', value: 1 },\n];\n\nconst navBarDropDownSelectedOption = navBarDropDownOptions[0];\n\nexport function NavigationBars() {\n  return (\n    <View styleName=\"vertical collapsed\">\n      <Stage title=\"Navbar / Solid\">\n        <NavBarStageContainer>\n          <NavigationBar\n            centerComponent={<Title>TITLE</Title>}\n          />\n        </NavBarStageContainer>\n      </Stage>\n      <Stage title=\"Navbar / Clear (Solid color)\">\n        <NavBarStageContainer style={{ backgroundColor: '#1a70d5', }}>\n          <NavigationBar\n            styleName=\"clear\"\n            centerComponent={<Title>TITLE</Title>}\n          />\n        </NavBarStageContainer>\n      </Stage>\n      <Stage title=\"Navbar / Clear (Image)\">\n        <Image\n          source={{ uri: 'https://shoutem.github.io/img/ui-toolkit/examples/image-3.png' }}\n          style={{\n            width: window.width,\n            height: 70,\n          }}\n        >\n          <NavigationBar\n            styleName=\"clear\"\n            centerComponent={<Title>TITLE</Title>}\n          />\n        </Image>\n      </Stage>\n      <Stage title=\"Navbar/ Fade (Gradient overlay + Solid color)\">\n        <NavBarStageContainer style={{ backgroundColor: '#1a70d5', }}>\n          <NavigationBar\n            styleName=\"fade clear\"\n            centerComponent={<Title>TITLE</Title>}\n          />\n        </NavBarStageContainer>\n      </Stage>\n      <Stage title=\"Navbar/ Fade (Gradient overlay + Image)\">\n        <Image\n          source={{uri: 'https://shoutem.github.io/img/ui-toolkit/examples/image-3.png' }}\n          style={{\n            width: window.width,\n            height: 70,\n          }}\n        >\n          <NavigationBar\n            styleName=\"fade clear\"\n            centerComponent={<Title>TITLE</Title>}\n          />\n        </Image>\n      </Stage>\n      <Heading styleName=\"sm-gutter\">Navigation bar variations</Heading>\n      <Stage title=\"Navbar + Drawer\">\n        <NavBarStageContainer>\n          <NavigationBar\n            leftComponent={(\n              <Button>\n                <Icon name=\"close\" />\n              </Button>\n            )}\n            centerComponent={<Title>TITLE</Title>}\n          />\n        </NavBarStageContainer>\n      </Stage>\n      <Stage title=\"Navbar + Picker\">\n        <NavBarStageContainer>\n          <NavigationBar\n            leftComponent={(\n              <Button>\n                <Icon name=\"sidebar\" />\n              </Button>\n            )}\n            centerComponent={<Title>TITLE</Title>}\n            rightComponent={<DropDownMenu\n              options={navBarDropDownOptions}\n              selectedOption={navBarDropDownSelectedOption}\n              titleProperty=\"name\"\n              valueProperty=\"value\"\n            />}\n          />\n        </NavBarStageContainer>\n      </Stage>\n      <Stage title=\"Navbar + Action\">\n        <NavBarStageContainer>\n          <NavigationBar\n            leftComponent={(\n              <Button>\n                <Icon name=\"sidebar\" />\n              </Button>\n            )}\n            centerComponent={<Title>TITLE</Title>}\n            rightComponent={(\n              <Button styleName=\"clear\">\n                <Text>List</Text>\n              </Button>\n            )}\n          />\n        </NavBarStageContainer>\n      </Stage>\n      <Stage title=\"Navbar + Icon\">\n        <NavBarStageContainer>\n          <NavigationBar\n            leftComponent={(\n              <Button>\n                <Icon name=\"sidebar\" />\n              </Button>\n            )}\n            centerComponent={<Title>TITLE</Title>}\n            rightComponent={(\n              <Button>\n                <Icon name=\"cart\" />\n              </Button>\n            )}\n          />\n        </NavBarStageContainer>\n      </Stage>\n      <Stage title=\"Navbar (Sublevel) + Icon\">\n        <NavBarStageContainer>\n          <NavigationBar\n            leftComponent={(\n              <Button>\n                <Icon name=\"back\" />\n              </Button>\n            )}\n            title=\"TITLE\"\n            share={{\n              link: 'http://shoutem.github.io',\n              text: 'This is the best',\n              title: 'Super cool UI Toolkit',\n            }}\n          />\n        </NavBarStageContainer>\n      </Stage>\n      <Stage title=\"Navbar (Sublevel) + Action (no border)\">\n        <NavBarStageContainer>\n          <NavigationBar\n            styleName=\"no-border\"\n            leftComponent={(\n              <Button>\n                <Icon name=\"back\" />\n              </Button>\n            )}\n            title=\"TITLE\"\n            share={{\n              link: 'http://shoutem.github.io',\n              text: 'This is the best',\n              title: 'Super cool UI Toolkit',\n            }}\n          />\n        </NavBarStageContainer>\n      </Stage>\n      <Stage title=\"Navbar (Sublevel) + Action\">\n        <NavBarStageContainer>\n          <NavigationBar\n            leftComponent={(\n              <Button>\n                <Icon name=\"back\" />\n              </Button>\n            )}\n            title=\"TITLE\"\n            rightComponent={(\n              <Button styleName=\"clear\">\n                <Text>Report</Text>\n              </Button>\n            )}\n          />\n        </NavBarStageContainer>\n      </Stage>\n      <Stage title=\"Navbar (Modal) + Icon\">\n        <NavBarStageContainer>\n          <NavigationBar\n            leftComponent={(\n              <Button>\n                <Icon name=\"close\" />\n              </Button>\n            )}\n            title=\"TITLE\"\n            share={{\n              link: 'http://shoutem.github.io',\n              text: 'This is the best',\n              title: 'Super cool UI Toolkit',\n            }}\n          />\n        </NavBarStageContainer>\n      </Stage>\n      <Stage title=\"Navbar (Modal) + Action\">\n        <NavBarStageContainer>\n          <NavigationBar\n            leftComponent={(\n              <Button>\n                <Icon name=\"close\" />\n              </Button>\n            )}\n            title=\"TITLE\"\n            rightComponent={(\n              <Button styleName=\"clear\">\n                <Text>Post</Text>\n              </Button>\n            )}\n          />\n        </NavBarStageContainer>\n      </Stage>\n      <Stage title=\"Navbar (Modal) + Action 2\">\n        <NavBarStageContainer>\n          <NavigationBar\n            leftComponent={(\n              <Button>\n                <Text>Cancel</Text>\n              </Button>\n            )}\n            title=\"TITLE\"\n            rightComponent={(\n              <Button>\n                <Text>Done</Text>\n              </Button>\n            )}\n          />\n        </NavBarStageContainer>\n      </Stage>\n      <Stage title=\"Navbar (Modal) + Action 2 (disabled)\">\n        <NavBarStageContainer>\n          <NavigationBar\n            leftComponent={(\n              <Button>\n                <Text>Cancel</Text>\n              </Button>\n            )}\n            title=\"TITLE\"\n            rightComponent={(\n              <Button styleName=\"muted\">\n                <Text>Done</Text>\n              </Button>\n            )}\n          />\n        </NavBarStageContainer>\n      </Stage>\n      <Stage title=\"Navbar / On primary color / back + share\">\n        <NavBarStageContainer style={{ backgroundColor: '#1a70d5', }}>\n          <NavigationBar\n            styleName=\"clear\"\n            leftComponent={(\n              <Button>\n                <Icon name=\"back\" />\n              </Button>\n            )}\n            title=\"TITLE\"\n            share={{\n              link: 'http://shoutem.github.io',\n              text: 'This is the best',\n              title: 'Super cool UI Toolkit',\n            }}\n          />\n        </NavBarStageContainer>\n      </Stage>\n    </View>\n  );\n}\n"]}]