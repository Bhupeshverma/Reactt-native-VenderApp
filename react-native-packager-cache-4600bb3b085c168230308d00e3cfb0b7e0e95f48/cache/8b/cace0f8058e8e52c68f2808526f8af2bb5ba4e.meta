["a5b971b801fee2ae431c4ce58522560b827acb8d","74809caf899f500ccf1dc03900a75f34bd6e4e95",["react","react-native","react-native-share","@shoutem/theme","./Button","./Icon"],[140,237,287,413,455,489],{"version":3,"sources":["/home/ziddi/Desktop/Bhupesh/venders/node_modules/@shoutem/ui/components/ShareButton.js"],"names":["USER_CANCELLED_ERROR","PropTypes","func","string","showErrorMessage","error","alert","ShareButton","props","onShare","bind","onError","title","message","url","open","catch","animationName","propTypes","defaultProps","StyledShareButton","undefined","virtual"],"mappings":";;;;;;;AAAA;;;;AAIA;;AAEA;;;;AAEA;;AAEA;;AACA;;AAEA,IAAMA,uBAAuB,oBAA7B;;uBAEyB,gBAAMC,S;IAAvBC,I,oBAAAA,I;IAAMC,M,oBAAAA,M;;;AAEd,IAAMC,mBAAmB,SAAnBA,gBAAmB,CAACC,KAAD,EAAW;AAClC,MAAIA,MAAMA,KAAN,KAAgBL,oBAApB,EAA0C;AACxC;AACD;;AAED,qBAAMM,KAAN,CACE,eADF,EAEED,MAAMA,KAFR;AAID,CATD;;IAmBME,W;;;AAkBJ,uBAAYC,KAAZ,EAAmB;AAAA;;AAAA,sIACXA,KADW;;AAGjB,UAAKC,OAAL,GAAe,MAAKA,OAAL,CAAaC,IAAb,OAAf;AAHiB;AAIlB;;;;8BAES;AAAA,mBACiC,KAAKF,KADtC;AAAA,UACAG,OADA,UACAA,OADA;AAAA,UACSC,KADT,UACSA,KADT;AAAA,UACgBC,OADhB,UACgBA,OADhB;AAAA,UACyBC,GADzB,UACyBA,GADzB;;;AAGR,iCAAMC,IAAN,CAAW;AACTH,oBADS;AAETC,wBAFS;AAGTC;AAHS,OAAX,EAIGE,KAJH,CAIS,UAACX,KAAD,EAAW;AAClBM,gBAAQN,KAAR;AACD,OAND;AAOD;;;6BAEQ;AAAA,UACCY,aADD,GACmB,KAAKT,KADxB,CACCS,aADD;;;AAGP,aACE;AAAA;AAAA;AACE,mBAAS,KAAKR;AADhB;AAGE,oDAAM,MAAK,OAAX,EAAmB,eAAeQ,aAAlC;AAHF,OADF;AAOD;;;4BA7CMC,S,GAAY;AAEjBD,iBAAed,MAFE;;AAIjBU,WAASV,MAJQ;;AAMjBQ,WAAST,IANQ;;AAQjBU,SAAOT,MARU;;AAUjBW,OAAKX;AAVY,C,SAaZgB,Y,GAAe;AACpBR,WAASP;AADW,C;;;AAmCxB,IAAMgB,oBAAoB,yBAAa,wBAAb,EAAuCC,SAAvC,EAAkDA,SAAlD,EACxB,EAAEC,SAAS,IAAX,EADwB,EACLf,WADK,CAA1B;;QAIuBA,W,GAArBa,iB","sourcesContent":["import React, {\n  Component,\n} from 'react';\n\nimport { Alert } from 'react-native';\n\nimport Share from 'react-native-share';\n\nimport { connectStyle } from '@shoutem/theme';\n\nimport { Button } from './Button';\nimport { Icon } from './Icon';\n\nconst USER_CANCELLED_ERROR = 'User did not share';\n\nconst { func, string } = React.PropTypes;\n\nconst showErrorMessage = (error) => {\n  if (error.error === USER_CANCELLED_ERROR) {\n    return;\n  }\n\n  Alert.alert(\n    'Sharing error',\n    error.error,\n  );\n};\n\n/**\n * The ShareButton is a virtual component that wraps a button with a share icon.\n * It puts the sharing logic in one place. It's used in the navigation bar in the toolkit,\n * but it can be reused anywhere.\n *\n * It should have the style of its underlying button. That's why it's not connected to style\n * or animation.\n */\nclass ShareButton extends Component {\n  static propTypes = {\n    // Animation name for share icon\n    animationName: string,\n    // Message to share\n    message: string,\n    // Called when there was a sharing error\n    onError: func,\n    // Title\n    title: string,\n    // Url to share\n    url: string,\n  };\n\n  static defaultProps = {\n    onError: showErrorMessage,\n  };\n\n  constructor(props) {\n    super(props);\n\n    this.onShare = this.onShare.bind(this);\n  }\n\n  onShare() {\n    const { onError, title, message, url } = this.props;\n\n    Share.open({\n      title,\n      message,\n      url,\n    }).catch((error) => {\n      onError(error);\n    });\n  }\n\n  render() {\n    const { animationName } = this.props;\n\n    return (\n      <Button\n        onPress={this.onShare}\n      >\n        <Icon name=\"share\" animationName={animationName} />\n      </Button>\n    );\n  }\n}\n\nconst StyledShareButton = connectStyle('shoutem.ui.ShareButton', undefined, undefined,\n  { virtual: true })(ShareButton);\n\nexport {\n  StyledShareButton as ShareButton,\n};\n"]}]