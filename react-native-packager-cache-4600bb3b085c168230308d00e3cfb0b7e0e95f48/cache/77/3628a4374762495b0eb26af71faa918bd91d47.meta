["0c1afba43846546ea782ce4efb03f1604c459999","45899b9cae64f42d93dfefdf978a7629fbb998de",["../actions/types","lodash","redux-persist/constants"],[646,690,788],{"version":3,"sources":["/home/ziddi/Desktop/Bhupesh/venders/src/reducers/savedDataReducer.js"],"names":["state","action","type","payload","savedDataReducer","uniqBy"],"mappings":";;;;kBAUe,YAA6B;AAAA,MAApBA,KAAoB,uEAAZ,EAAY;AAAA,MAARC,MAAQ;;AAC1C,UAAQA,OAAOC,IAAf;AACE;AACE,aAAOD,OAAOE,OAAP,CAAeC,gBAAf,IAAmC,EAA1C;AACF;AACE,aAAO,EAAP;AACF;AACE,aAAO,EAAP;AACF;AACE,aAAO,iBAAEC,MAAF,EACLJ,OAAOE,OADF,wCACcH,KADd,IAEJ,YAFI,CAAP;AAGF;AACE,aAAOA,KAAP;AAZJ;AAcD,C;;AAzBD;;AAKA;;;;AACA","sourcesContent":["import {\n  SAVED_PRODUCTS,\n  CLEAR_SAVED_PRODUCTS,\n  CHANGE_SAVED_DATA_STATUS\n} from '../actions/types';\nimport _ from 'lodash';\nimport { REHYDRATE } from 'redux-persist/constants';\n\n\n\nexport default function(state = [], action) {\n  switch (action.type) {\n    case REHYDRATE:\n      return action.payload.savedDataReducer || [];\n    case CLEAR_SAVED_PRODUCTS:\n      return [];\n    case CHANGE_SAVED_DATA_STATUS:\n      return [];\n    case SAVED_PRODUCTS:\n      return _.uniqBy([\n        action.payload, ...state\n      ], 'product_id');\n    default:\n      return state;\n  }\n}\n"]}]